{
    "name": "Name Generator Extension",
    "description": "Generates a list of random names of various themes and categories; useful for GMs and players of TTRPGS.",
    "version": "0.0.1",
    "content_security_policy": {
        "extension_pages": "script-src 'self' ; object-src 'self'"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icons/png/orange/orange_gear_16.png", 
            "19": "icons/png/orange/orange_gear_19.png",
            "32": "icons/png/orange/orange_gear_32.png", 
            "38": "icons/png/orange/orange_gear_38.png"
        }
    },
    "icons": {
        "16": "icons/png/orange/orange_gear_16.png", 
        "19": "icons/png/orange/orange_gear_19.png",
        "24": "icons/png/orange/orange_gear_24.png", 
        "32": "icons/png/orange/orange_gear_32.png",
        "38": "icons/png/orange/orange_gear_38.png", 
        "48": "icons/png/orange/orange_gear_48.png",
        "128": "icons/png/orange/orange_gear_128.png", 
        "1024": "icons/png/orange/orange_gear_1024.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "manifest_version": 3
}